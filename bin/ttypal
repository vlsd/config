    echo -en "\e]P1CC0000" # Red
    echo -en "\e]P24E9A06" 
    echo -en "\e]P3C4A000"
    echo -en "\e]P43465A4"
    echo -en "\e]P575507B"
    echo -en "\e]P606989A"
    echo -en "\e]P7D3D7CF"
    echo -en "\e]P8555753"
    echo -en "\e]P9EF2929"
    echo -en "\e]PA8AE234"
    echo -en "\e]PBC4A000"
    echo -en "\e]PC729FCF"
    echo -en "\e]PDAD7FA8"
    echo -en "\e]PE34E2E2" # Bright Cyan
    echo -en "\e]PFEEEEEC" # Bright White
    clear # Clear artifacts
fi

